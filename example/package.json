    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "22.4.2",
    "react-test-renderer": "^16.3.0-alpha.1",
    "webpack": "^4.8.3",
